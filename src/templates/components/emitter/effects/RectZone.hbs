{{#ifCond type 'emit'}}
	emitterZoneEmitter.addInitialize(new Proton.Position(new Proton.RectZone({{x}}, {{y}}, {{width}}, {{height}})));
{{else}}
	emitterZoneEmitter.addBehaviour(new Proton.CrossZone(new Proton.RectZone({{x}}, {{y}}, {{width}}, {{height}}),
		Proton.CrossZone.CROSS_TYPES.{{type}}));
{{/ifCond}}
