{{#ifCond type 'emit'}}
	emitterZoneEmitter.addInitialize(new Quark.Position(new Quark.RectZone({{x}}, {{y}}, {{width}}, {{height}})));
{{else}}
	emitterZoneEmitter.addBehaviour(new Quark.CrossZone(new Quark.RectZone({{x}}, {{y}}, {{width}}, {{height}}),
		Quark.CrossZone.CROSS_TYPES.{{type}}));
{{/ifCond}}
