{{#ifCond enabled true}}
var godrayFilter = new Filters.GodrayFilter();;
if (this.filters.length < 1) {
  this.filters.push(godrayFilter);
}

this.filters.forEach(function(filter) {
  if (filter === godrayFilter) {
    filter.time = {{time}}
    filter.angle = {{angle}}
    filter.gain = {{gain}}
    filter.lacunarity = {{lacunarity}}
  }else {
    that.filters.push(godrayFilter)
  }
});
{{/ifCond}}