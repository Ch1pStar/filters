{{#ifCond enabled true}}
var colorReplaceFilter = new Filters.ColorReplaceFilter();;
if (this.filters.length < 1) {
  this.filters.push(colorReplaceFilter);
}
var that = this;
this.filters.forEach(function(filter) {
  if (filter === colorReplaceFilter) {
    filter.originalColor = '{{originalColor}}''
    filter.newColor = '{{newColor}}'
    filter.epsilon = {{epsilon}}
  }else {
    that.filters.push(colorReplaceFilter)
  }
});
{{/ifCond}}