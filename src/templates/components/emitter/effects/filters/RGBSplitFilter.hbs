{{#ifCond enabled true}}
var RGBSplitFilter = new Filters.RGBSplitFilter();;
if (this.filters.length < 1) {
  this.filters.push(RGBSplitFilter);
}
var that = this;
this.filters.forEach(function(filter) {
  if (filter === RGBSplitFilter) {
    filter.redX = {{redX}};
    filter.redY = {{redY}}; 
    filter.blueX = {{blueX}}; 
    filter.blueY = {{blueY}}; 
    filter.greenX = {{greenX}}; 
    filter.greenY = {{greenY}};    
  }else {
    that.filters.push(RGBSplitFilter)
  }
});
{{/ifCond}}