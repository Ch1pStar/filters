{{#ifCond enabled true}}
var blurFilter = new Filters.BlurFilter();;
if (this.filters.length < 1) {
  this.filters.push(blurFilter);
}
var that = this;
this.filters.forEach(function(filter) {
  if (filter === blurFilter) {
    filter.size = {{blur}};
    filter.size = {{quality}};
  }else {
    that.filters.push(blurFilter)
  }
});
{{/ifCond}}