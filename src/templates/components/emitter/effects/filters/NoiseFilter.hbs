var noiseFilter = new Filters.NoiseFilter();;
if (this.filters.length < 1) {
  this.filters.push(noiseFilter);
}
var that = this;
this.filters.forEach(function(filter) {
  if (filter === noiseFilter) {
    filter.noise = {{noise}};
    filter.seed = {{seed}};    
  }else {
    that.filters.push(noiseFilter)
  }
});