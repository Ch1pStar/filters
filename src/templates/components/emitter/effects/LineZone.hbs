{{#ifCond type 'emit'}}
emitterZoneEmitter.addInitialize(new Proton.Position(new Proton.LineZone({{x1}}, {{y1}}, {{x2}}, {{y2}})));
{{else}}
emitterZoneEmitter.addBehaviour(new Proton.CrossZone(new Proton.LineZone({{x1}}, {{y1}}, {{x2}}, {{y2}}, {{direction}}),
Proton.CrossZone.CROSS_TYPES.{{type}}));
{{/ifCond}}
