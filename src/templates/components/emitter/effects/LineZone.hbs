{{#ifCond type 'emit'}}
emitterZoneEmitter.addInitialize(new Quark.Position(new Quark.LineZone({{x1}}, {{y1}}, {{x2}}, {{y2}})));
{{else}}
emitterZoneEmitter.addBehaviour(new Quark.CrossZone(new Quark.LineZone({{x1}}, {{y1}}, {{x2}}, {{y2}}, {{direction}}),
Quark.CrossZone.CROSS_TYPES.{{type}}));
{{/ifCond}}
